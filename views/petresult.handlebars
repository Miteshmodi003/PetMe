{{> navigationbar}}
<div class="container">
    <div class="row">
        {{#each pets}}
        <div class="col-xs-3"></div>
        <div class="col-xs-6 col-md-6 col-lg-4 mt-3">
            <div class="card" style="width: 18rem;">
                <div class="card-body">
                    <p class="card-text"><strong>{{this.dataValues.pet_name}}</strong></p>
                    <hr>
                    <p class="card-text"><strong>Age:</strong> {{this.dataValues.pet_name}} <br>
                        <strong>Breed: </strong>{{this.dataValues.breed}}
                        <br>
                        <strong>Zipcode:</strong>{{this.dataValues.zip_code}}</p>
                    <p class="card-text">Maiores vitae possimus illo illum aliquid obcaecati ipsa. Illum rem eius eum
                        corrupti perferendis placeat nulla iste voluptatum esse fugiat, reiciendis odit!</p>
                </div>
            </div>
        </div>
        <div class="col-xs-3"></div>
        {{/each}}
    </div>
    

</div>